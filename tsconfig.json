{
  "compilerOptions": {
    "target": "esnext",
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "allowJs": true,
    "checkJs": true,
    "paths": {
      "markdown-it-math": ["./index.js"]
    }
  },
  "include": ["index.js", "no-default-renderer.js", "temml.js", "src/**/*.js"],
  "exclude": ["coverage/"]
}
